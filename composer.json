    "authors": [
        {
            "name": "Vitaliy",
            "email": "tsoyvetal@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "slim/slim": "^4.14",
        "slim/psr7": "^1.7",
        "slim/http": "^1.4",
        "php-di/php-di": "^7.0",
        "symfony/var-dumper": "^7.2",
        "illuminate/support": "^12.11",
        "slim/flash": "^0.4.0",
        "nesbot/carbon": "^3.9",
        "vlucas/valitron": "^1.4",
        "vlucas/phpdotenv": "^5.6",
        "guzzlehttp/guzzle": "^7.9",
        "ext-pdo": "*",
        "imangazaliev/didom": "^2.0",
        "slim/php-view": "^3.4"
    },
    "scripts": {
        "lint": "phpcs --standard=PSR12 src/ public/",
